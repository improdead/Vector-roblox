########################################
# Vector Web â€” .env.example
# Copy to .env.local and fill required values
########################################

# Provider
VECTOR_DEFAULT_PROVIDER=openrouter
VECTOR_USE_OPENROUTER=1
OPENROUTER_API_KEY=
OPENROUTER_MODEL=openai/gpt-4o-mini
OPENROUTER_TIMEOUT_MS=30000
OPENROUTER_MAX_RETRIES=3
OPENROUTER_RETRY_DELAY_MS=1000
OPENROUTER_RETRY_MAX_MS=10000
GEMINI_API_KEY=
GEMINI_MODEL=gemini-2.5-flash
GEMINI_API_BASE_URL=
GEMINI_TIMEOUT_MS=30000
VECTOR_MAX_TURNS=4
VECTOR_WORKSPACE_ROOT=
VECTOR_PROBLEMS_FILE=

# Planning behavior
# Require an explicit <start_plan> before performing any action tools (geometry/code edits) in agent mode.
VECTOR_REQUIRE_PLAN=0

# Catalog (optional)
# Leave CATALOG_API_URL empty (or set to "roblox") to use the built-in Roblox public catalog search.
# Point CATALOG_API_URL at a proxy service if you need custom filtering or caching.
# Set CATALOG_DISABLE_SEARCH=1 to force manual instance creation in workflows.
CATALOG_API_URL=
CATALOG_API_KEY=
CATALOG_TIMEOUT_MS=15000
CATALOG_DISABLE_SEARCH=0

# 3D generation & upload (optional for dev)
MESHY_API_KEY=
MESHY_API_URL=https://api.meshy.ai/openapi/v2/text-to-3d
MESHY_TIMEOUT_MS=60000
ROBLOX_OPEN_CLOUD_API_KEY=
ROBLOX_UNIVERSE_ID=
ROBLOX_GROUP_ID=

# Persistence (optional)
VECTOR_DB=
DATABASE_URL=

# Web client (optional override)
NEXT_PUBLIC_BACKEND_URL=
# Copy this file to .env.local or .env and fill in your secrets.
# Do NOT commit real keys.

# Optional: enable Plan/Act two-step context tool execution
VECTOR_PLAN_ACT=0
